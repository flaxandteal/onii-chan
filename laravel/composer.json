{
  "require": {
    "laravel/framework": "4.2.*",
    "mitch/laravel-doctrine": "~0.3",
    "heybigname/event-dispatcher": "~1.1",
    "rhumsaa/uuid": "~2.7",
    "beberlei/assert": "~2.0"
  },
  "require-dev": {
    "mockery/mockery": "~0.8",
    "phpunit/phpunit": "4.0"
  },
  "autoload": {
    "psr-4": {
      "OniiChan\\": "src/lib"
    },
    "classmap": [
      "app/commands",
      "app/controllers",
      "app/models",
      "app/database/migrations",
      "app/database/seeds",
      "src/tests/TestCase.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "OniiChan\\Test\\": "src/tests"
    }
  },
  "scripts": {
    "post-install-cmd": [
      "php artisan clear-compiled",
      "php artisan optimize"
    ],
    "post-update-cmd": [
      "php artisan clear-compiled",
      "php artisan optimize"
    ],
    "post-create-project-cmd": [
      "php artisan key:generate"
    ]
  },
  "config": {
    "preferred-install": "dist"
  },
  "minimum-stability": "dev"
}
